{
  "name": "secretin-app",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://secret-in.me",
  "main": "electron.js",
  "devDependencies": {
    "babel-eslint": "7.1.1",
    "electron": "^1.4.13",
    "electron-packager": "^8.4.0",
    "eslint": "3.12.2",
    "eslint-config-airbnb": "13.0.0",
    "eslint-import-resolver-webpack": "0.8.0",
    "eslint-plugin-flowtype": "2.29.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.8.0",
    "gh-pages": "^0.12.0",
    "node-sass": "4.0.0",
    "raw-loader": "0.5.1",
    "react-scripts": "0.8.4",
    "webpack": "1.14.0"
  },
  "dependencies": {
    "alt": "0.18.6",
    "alt-utils": "1.0.0",
    "classnames": "2.2.5",
    "copy-to-clipboard": "3.0.5",
    "immutable": "3.8.1",
    "lodash": "4.17.2",
    "moment": "2.17.1",
    "mousetrap": "1.6.0",
    "qrcode.react": "^0.6.1",
    "react": "15.4.1",
    "react-bootstrap": "0.30.7",
    "react-dnd": "2.1.4",
    "react-dnd-html5-backend": "2.1.2",
    "react-dom": "15.4.1",
    "react-overlays": "0.6.10",
    "react-router": "4.0.0-alpha.6",
    "secretin": "1.*.*",
    "url-join": "1.1.0",
    "uuid": "3.0.1"
  },
  "scripts": {
    "deploy": "scripts/deploy.sh",
    "start": "yarn run build-css && yarn run watch-css & NODE_PATH=./src  react-scripts start",
    "build": "yarn run build-css && NODE_PATH=./src  react-scripts build",
    "build-css": "node-sass src/stylesheets/application.sass src/index.css",
    "watch-css": "node-sass src/stylesheets/application.sass src/index.css -w",
    "electron": "yarn run build && electron-packager ./ --platform=all --arch=x64 --asar --overwrite --version=1.4.13 --ignore=scripts --ignore=.gitignore --ignore=yarn.lock --ignore=.travis.yml --ignore=.*.zip --ignore=public --ignore=.nvmrc --ignore=.eslintrc.js --ignore=node_modules --ignore=src",
    "lint": "eslint src/",
    "test": "yarn run lint && react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "./.eslintrc.js"
  }
}
